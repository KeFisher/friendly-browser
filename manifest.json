{
    "name": "FriendlyBrowser",
    "version": "1.0",
    "description": "Censor profanity and terms that are not politically correct.",
    "manifest_version": 2,
    "author": "Caiden Pyle",
    
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ],
}
